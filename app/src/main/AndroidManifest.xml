<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.authenticate">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Google_Reg_Activity"></activity>
        <activity android:name=".Teacher_Subject" />
        <activity android:name=".Teacher_details" />
        <activity
            android:name=".SetAttendance"
            android:label="@string/title_activity_set_attendance"
            android:parentActivityName=".Teacher"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Teacher" />
        <activity
            android:name=".subjects"
            android:parentActivityName=".Student_Main" />
        <activity
            android:name=".Student_Main"
            android:label="@string/title_activity_student__main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Student_Activity" />
        <activity android:name=".Reg_Activity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SplashActivity" />
    </application>

</manifest>